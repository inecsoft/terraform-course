{
  "name": "@winglibs/openai",
  "description": "OpenAI library for Wing",
  "version": "0.0.5",
  "repository": {
    "type": "git",
    "url": "https://github.com/winglang/winglibs.git",
    "directory": "openai"
  },
  "author": {
    "email": "shaib@wing.cloud",
    "name": "Shai Ber"
  },
  "license": "MIT",
  "wing": true,
  "peerDependencies": {
    "openai": "^4.28.4"
  },
  "files": [
    "$lib",
    "!$lib/.manifest",
    "**/*.js",
    "**/*.cjs",
    "**/*.mjs",
    "**/*.ts",
    "**/*.cts",
    "**/*.mts",
    "**/*.w",
    "README*",
    "LICENSE*",
    "!target",
    "!**/main.w",
    "!**/*.main.w",
    "!**/*.test.w"
  ],
  "main": "$lib/.wing/preflight.cjs",
  "keywords": [
    "winglang"
  ],
  "engines": {
    "wing": "*"
  }
}
