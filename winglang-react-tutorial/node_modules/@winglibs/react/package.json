{
  "name": "@winglibs/react",
  "description": "React library for Wing",
  "version": "0.1.0",
  "author": {
    "name": "Meir Elbaz",
    "email": "maornet@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/winglang/winglibs.git",
    "directory": "react"
  },
  "wing": true,
  "license": "MIT",
  "peerDependencies": {
    "finalhandler": "^1.2.0",
    "get-port": "^7.1.0",
    "serve-static": "^1.15.0",
    "tree-kill": "^1.2.2"
  },
  "files": [
    "$lib",
    "!$lib/.manifest",
    "**/*.js",
    "**/*.cjs",
    "**/*.mjs",
    "**/*.ts",
    "**/*.cts",
    "**/*.mts",
    "**/*.w",
    "README*",
    "LICENSE*",
    "!target",
    "!**/main.w",
    "!**/*.main.w",
    "!**/*.test.w"
  ],
  "main": "$lib/.wing/preflight.cjs",
  "keywords": [
    "winglang"
  ],
  "engines": {
    "wing": "*"
  }
}
